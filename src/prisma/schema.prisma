generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Profile {
  id           Int     @id @default(autoincrement())
  github_login String
  name         String?
}

model Search {
  id       Int      @id @default(autoincrement())
  date     DateTime @default(now())
  location String   @default("Paris")
  terms    String?
}
