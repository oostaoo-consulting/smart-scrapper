generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Profile {
  id    Int     @id @default(autoincrement())
  login String
  name  String? @default("")
}

model Search {
  id       Int      @id @default(autoincrement())
  date     DateTime @default(now())
  location String   @default("Paris")
  terms    String?  @default("")
}
