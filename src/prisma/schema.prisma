generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("PG_URL")
}

model profile {
  id           Int    @id @default(autoincrement())
  github_login String
}

model search {
  id       Int     @id @default(autoincrement())
  location String  @default("Paris")
  terms    String?
}
